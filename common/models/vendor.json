{
    "name": "Vendor",
    "plural": "vendors",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "name": {
            "type": "string"
        },
        "displayName": {
            "type": "string"
        },
        "legalName": {
            "type": "string"
        },
        "description": {
            "type": "String"
        },
        "email": {
            "type": "string"
        },
        "addressLine1": {
            "type": "string"
        },
        "addressLine2": {
            "type": "string"
        },
        "city": {
            "type": "string"
        },
        "pincode": {
            "type": "number",
            "min": 6,
            "max": 6
        },
        "country": {
            "type": "string",
            "default": "india"
        },
        "mobile": {
            "type": "number",
            "length": 10
        },
        "alternateMobile": {
            "type": "number",
            "length": 10
        },
        "landline": {
            "type": "number"
        },
        "fax": {
            "type": "number"
        },
        "website": {
            "type": "string"
        },
        "googlePlayLink": {
            "type": "string"
        },
        "appStroreLink": {
            "type": "string"
        },
        "facebookPageLink": {
            "type": "string"
        },
        "twitterHandleLink": {
            "type": "string"
        },
        "pinterestPageLink": {
            "type": "string"
        },
        "tumblrPageLink": {
            "type": "string"
        },
        "linkedinPageLink": {
            "type": "string"
        },
        "isInternal": {
            "type": "Boolean",
            "default": false
        }
    },
    "validations": [],
    "relations": {
        "stores": {
            "type": "hasMany",
            "model": "Store"
        },
        "address": {
            "type": "embedsOne",
            "model": "Address",
            "property": "address",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        },
        "social": {
            "type": "embedsOne",
            "model": "Social",
            "property": "social",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        },
        "contact": {
            "type": "embedsOne",
            "model": "Contact",
            "property": "contact",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        },
        "banking": {
            "type": "embedsOne",
            "model": "Banking",
            "property": "banking",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        },
        "online": {
            "type": "embedsOne",
            "model": "Online",
            "property": "online",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        },
        "coupons": {
            "type": "hasMany",
            "model": "Coupon",
            "property": "coupons"
        },
        "deals": {
            "type": "hasMany",
            "model": "Deal",
            "property": "deals"
        },
        "brandLogo": {
            "type": "embedsOne",
            "model": "Image",
            "property": "brandLogo",
            "options": {
                "validate": true,
                "forceId": true,
                "presistent": true
            }
        }
    },
    "acls": [],
    "methods": {}
}