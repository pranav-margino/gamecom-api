    },
    "mixins": {
        "TimeStamp": true
    },
    "properties": {
        "context": {
            "type": "String"
        },
        "userId": {
            "type": "String"
        },
        "favouriteId": {
            "type": "String"
        },
        "values": {
            "type": ["number"]

        },
        "hasRecent": {
            "type": "Boolean",
